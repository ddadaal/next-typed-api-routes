{
  "routes": {
    "LoginSchema": {
      "type": "object",
      "properties": {
        "method": {
          "type": "string",
          "const": "GET"
        },
        "query": {
          "$ref": "LoginInfo#"
        },
        "responses": {
          "type": "object",
          "properties": {
            "200": {
              "type": "object",
              "properties": {
                "token": {
                  "type": "string"
                }
              },
              "required": [
                "token"
              ],
              "additionalProperties": false
            },
            "401": {
              "type": "object",
              "properties": {
                "reason": {
                  "type": "string",
                  "enum": [
                    "NotExists",
                    "WrongPassword"
                  ]
                }
              },
              "required": [
                "reason"
              ],
              "additionalProperties": false
            }
          },
          "required": [
            "200",
            "401"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "method",
        "query",
        "responses"
      ],
      "additionalProperties": false,
      "$id": "LoginSchema",
      "title": "LoginSchema"
    }
  },
  "models": {
    "LoginInfo": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "description": "The username",
          "format": "idn-email",
          "maxLength": 20
        },
        "password": {
          "type": "string",
          "description": "The password",
          "minLength": 8
        }
      },
      "required": [
        "username",
        "password"
      ],
      "additionalProperties": false,
      "$id": "LoginInfo",
      "title": "LoginInfo"
    }
  }
}