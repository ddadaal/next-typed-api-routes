{
  "compilerOptions": {
    "jsx": "react",
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "declaration": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "strict": false,
    "newLine": "lf",
    "incremental": true
  }
}
