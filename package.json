{
  "name": "next-typed-api-routes",
  "version": "1.0.0",
  "description": "Make Next.js API Routes strongly typed with routes and clients type completion and auto-generated parameter validation",
  "source": "src/index.ts",
  "exports": "./dist/index.modern.js",
  "main": "dist/index.cjs",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ntar": "dist/cli/index.js"
  },
  "scripts": {
    "build-lib": "microbundle src/index.ts",
    "build-cli": "tsc --project tsconfig.cli.json",
    "build": "rimraf dist && npm run build-lib && npm run build-cli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddadaal/next-typed-api-routes.git"
  },
  "keywords": [
    "next",
    "typescript"
  ],
  "author": "Chen Junda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ddadaal/next-typed-api-routes/issues"
  },
  "homepage": "https://github.com/ddadaal/next-typed-api-routes#readme",
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/jest": "27.0.2",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint-plugin-react": "7.26.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "ts-json-schema-generator": "^0.95.0",
    "yargs": "^17.2.1",
    "eslint": "^7.32.0"
  },
  "peerDependencies": {
    "next": "^11.1.2"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "fast-json-stringify": "^2.7.10",
    "next": "^11.1.2"
  }
}
