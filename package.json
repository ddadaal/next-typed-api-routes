{
  "name": "@ddadaal/next-typed-api-routes",
  "version": "0.1.10",
  "description": "Make Next.js API Routes strongly typed with routes and clients type completion and auto-generated parameter validation",
  "bin": {
    "ntar": "lib/cli/index.js"
  },
  "scripts": {
    "build-client": "rollup --config rollup.client.ts --configPlugin typescript",
    "build-server": "rollup --config rollup.server.ts --configPlugin typescript",
    "build-cli": "tsc --project tsconfig.cli.json",
    "build": "rimraf lib && npm run build-cli && npm run build-client && npm run build-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddadaal/next-typed-api-routes.git"
  },
  "keywords": [
    "next",
    "typescript"
  ],
  "author": {
    "name": "Chen Junda",
    "email": "ddadaal@outlook.com",
    "url": "https://ddadaal.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ddadaal/next-typed-api-routes/issues"
  },
  "homepage": "https://github.com/ddadaal/next-typed-api-routes#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/eslint": "^7.28.0",
    "@types/jest": "27.0.2",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "7.26.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.1"
  },
  "peerDependencies": {
    "next": ">=11.x"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "ajv-formats-draft2019": "^1.6.1",
    "fast-json-stringify": "^2.7.10",
    "long": "^4.0.0",
    "ts-json-schema-generator": "^0.95.0",
    "typescript": "^4.4.3",
    "yargs": "^17.2.1"
  }
}
